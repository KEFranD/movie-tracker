<%= simple_form_for @list, html: { class: "custom-form" } do |f| %>
  <div class="form-group">
    <%= f.label :name, class: "form-label" %>
    <%= f.text_field :name, class: "form-control" %>
    <%= f.input :photo, as: :file %>
  </div>
    <%= f.submit "Submit", class: "form--button" %>
<% end %>
